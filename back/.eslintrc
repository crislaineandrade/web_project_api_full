{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "import/prefer-default-export": "off",
    "import/extensions": [
      "never"
    ],
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  }
}